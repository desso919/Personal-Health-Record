<UserControl x:Class="Personal.Health.Care.DesktopApp.Pages.NavigationTabs.VisitationsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <!-- TODO: set @SelectedSource -->
        <mui:ModernTab Layout="List">
            <mui:ModernTab.Links>
                <!-- TODO: set @Source -->
                <mui:Link DisplayName="Today" />
                <mui:Link DisplayName="This Week" />
                <mui:Link DisplayName="This Month" />
                <mui:Link DisplayName="This Year" />
                <mui:Link DisplayName="See All" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
