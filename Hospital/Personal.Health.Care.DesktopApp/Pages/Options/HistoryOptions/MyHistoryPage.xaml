<UserControl x:Class="Personal.Health.Care.DesktopApp.Pages.Options.HistoryOptions.MyHistoryPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <!-- TODO: set @SelectedSource -->
        <mui:ModernTab Layout="List" SelectedSource="/Pages/Views/HistoryView.xaml" >
            <mui:ModernTab.Links>
                <!-- TODO: set @Source -->
                <mui:Link DisplayName="Yesterday" Source="/Pages/Views/HistoryView.xaml" />
                <mui:Link DisplayName="Last Week" Source="/Pages/Views/HistoryView.xaml" />
                <mui:Link DisplayName="Last Month" Source="/Pages/Views/HistoryView.xaml" />
                <mui:Link DisplayName="Last Year" Source="/Pages/Views/HistoryView.xaml"  />
                <mui:Link DisplayName="See All" Source="/Pages/Views/HistoryView.xaml" />
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
