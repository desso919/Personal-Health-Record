<UserControl x:Class="Personal.Health.Care.DesktopApp.Pages.Home"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <Frame Name="MyFrame"/>
        <mui:ModernTab Layout="Tab"  SelectedSource="/Pages/NavigationTabs/VisitationsPage.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Profile"  Source="/Pages/NavigationTabs/ProfilePage.xaml" />
                <mui:Link DisplayName="Medical Histoy"  Source="/Pages/NavigationTabs/HistoryPage.xaml" />
                <mui:Link DisplayName="Scheduled Visitations" Source="/Pages/NavigationTabs/VisitationsPage.xaml"/>
                <mui:Link DisplayName="Recommended Visitations" Source="/Pages/NavigationTabs/RecommendedVisitationsPage.xaml"/>
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
